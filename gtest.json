{
    "problemMatcher": [
        {
            "owner": "clang",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):\\sFailure",
                    "file": 1,
                    "line": 2
                },
                {
                    "regexp": "(.+:.+)",
                    "message": 1,
                    "loop": true
                }
            ]
        }
    ]
}
