{
    "problemMatcher": [
        {
            "owner": "clang",
            "pattern": [
                {
                    "regexp": "^(.+):(\\d+):\\sFailure$",
                    "file": 1,
                    "line": 2
                },
                {
                    "regexp": "^([a-zA-Z].+)$",
                    "message": 1
                }
            ]
        }
    ]
}
